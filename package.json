{
  "name": "react_native_boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && npx react-native run-android",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "generate": "plop"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/datetimepicker": "^2.1.0",
    "@react-native-community/geolocation": "1.4.2",
    "axios": "^0.19.0",
    "d3-shape": "^1.3.7",
    "i18n-js": "^3.0.11",
    "lodash.throttle": "4.1.1",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "ramda": "0.26.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-image-picker": "^1.1.0",
    "react-native-keychain": "3.1.3",
    "react-native-localize": "^1.0.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-size-matters": "^0.3.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-sqlite-storage": "^5.0.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-tiny-toast": "^1.0.7",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-hooks": "^1.1.0",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-sqlite-storage": "^1.0.4",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jsdom": "^16.2.1",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "^0.56.0",
    "plop": "^2.6.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.13.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "automock": false,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!android/**",
      "!ios/**",
      "!assets/**"
    ],
    "coverageDirectory": "jest_coverage",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community/|@?react-navigation|static-container)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./jest-setup.js",
      "./__mocks__/react-native-localize.js",
      "./__mocks__/geolocation.js"
    ],
    "moduleNameMapper": {}
  },
  "globals": {
    "shallow": true,
    "render": true,
    "wrapper": true,
    "mount": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
